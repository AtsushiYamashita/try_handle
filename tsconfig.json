{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "pretty": true,
    "newLine": "lf",
    "outDir": "dist/back",
    "baseUrl": "./",
    "rootDirs": [
      "./src/back/"
    ],
    "types": [
      "node"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {},
    "lib": []
  },
  "include": [
    "./src/back/"
  ],
  "exclude": [
    "node_modules",
    "./script/",
    "./tests/"
  ]
}
